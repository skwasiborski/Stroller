{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "name": "blobout",
      "type": "blob",
      "path": "data/{DateTime}.jpg",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ]
}